{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "tauri-sandbox",
  "version": "0.1.0",
  "identifier": "com.tauri-sandbox.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "visible": false,
        "url": "/"
      },
      {
        "label": "windows1",
        "url": "win1",
        "width": 320,
        "height": 240,
        "x": 0,
        "y": 0,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "windowEffects": null
      },
      {
        "label": "windows2",
        "url": "win2",
        "width": 320,
        "height": 240,
        "x": 325,
        "y": 0,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "windowEffects": null
      },
      {
        "label": "windows3",
        "url": "win3",
        "width": 320,
        "height": 240,
        "x": 650,
        "y": 0,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "windowEffects": null
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
